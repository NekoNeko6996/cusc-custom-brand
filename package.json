{
  "name": "@nekoneko6996/cusc-custom-brand",
  "version": "1.0.1",
  "description": "CUSC custom brand components (Header/Footer) for Open edX MFEs",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "prepare": "npm run build",
    "release": "npm publish --access public"
  },
  "peerDependencies": {
    "react": ">=17 <19",
    "react-dom": ">=17 <19",
    "@openedx/paragon": ">=20"
  },
  "devDependencies": {
    "typescript": "^5.6.0",
    "tsup": "^8.3.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0"
  }
}
