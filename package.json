{
  "name": "@nekoneko6996/cusc-custom-brand",
  "version": "1.0.17",
  "description": "CUSC custom brand components (Header/Footer) for Open edX MFEs",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.js --format esm,cjs --clean",
    "prepare": "npm run build",
    "release": "npm publish --access public"
  },
  "peerDependencies": {
    "react": ">=17 <19",
    "react-dom": ">=17 <19",
    "@openedx/paragon": ">=20"
  },
  "devDependencies": {
    "tsup": "^8.3.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
